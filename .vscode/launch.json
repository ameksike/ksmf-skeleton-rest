    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "WEB",
            "program": "${workspaceFolder}/node_modules/ksmf/bin/cli.js",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "args": ["run", "SrvAPI", "mito", "lete"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CURRENT",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
    ]
}