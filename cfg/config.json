{
	"default": {
		"from": "env",
		"db": {
			"host": "DB_HOST",
			"port": "DB_PORT",
			"database": "DB_NAME",
			"username": "DB_USER",
			"password": "DB_PASS",
			"dialect": "DB_DIALECT",
			"protocol": "mysql"
		},
		"import": [
			"cfg/constant.json",
			"cfg/log.json"
		]
	},
	"production": {
		"from": "env",
		"db": {
			"url": "DATABASE_AWS",
			"dialect": "postgres",
			"dialectOptions": {
				"ssl": {
					"rejectUnauthorized": false
				}
			}
		}
	},
	"test": {
		"dialect": "sqlite",
		"storage": ":memory:",
		"logging": false
	}
}