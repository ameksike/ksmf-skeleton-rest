{
	"default": {
		"from": "env",
		"db": {
			"port": "3303",
			"host": "localhost",
			"database": "DB_NAME",
			"username": "DB_USER",
			"password": "DB_PASS",
			"dialect": "mysql",
			"protocol": "mysql"
		},
		"import": [
			"cfg/constant.json",
			"cfg/log.json"
		]
	},
	"production": {
		"from": "env",
		"db": {
			"url": "DATABASE_AWS",
			"dialect": "postgres",
			"dialectOptions": {
				"ssl": {
					"rejectUnauthorized": false
				}
			}
		}
	},
	"test": {
		"dialect": "sqlite",
		"storage": ":memory:",
		"logging": false
	}
}