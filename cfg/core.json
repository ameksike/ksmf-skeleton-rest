{
    "development": {
        "port": 3005,
        "host": "localhost",
        "protocol": "http",
        "log": 2,
        "module": {
            "load": [
                "demo",
                "app"
            ]
        },
        "event": {
            "onInitConfig": [
                "dao.wrapper"
            ],
            "onInitModules": [
                "dao.wrapper"
            ],
            "onLoadModule": [
                "dao.wrapper"
            ]
        },
        "helper": {
            "sequelize": {
                "name": "ksmf",
                "type": "lib",
                "namespace": "dao.Sequelize",
                "dependency": {
                    "helper": "helper"
                }
            },
            "dao": {
                "name": "ksmf",
                "type": "lib",
                "namespace": "dao.Sequelize",
                "dependency": {
                    "helper": "helper"
                }
            },
            "dao.wrapper": {
                "name": "ksmf",
                "type": "lib",
                "params": {
                    "exclude": [
                        "forest"
                    ]
                },
                "namespace": "dao.Wrapper",
                "dependency": {
                    "helper": "helper"
                }
            },
            "logger": {
                "name": "ksmf",
                "type": "lib",
                "namespace": "app.Logger"
            },
            "MyAPI": {
                "path": "service",
                "module": "app"
            }
        }
    },
    "production": {
        "port": 3006,
        "host": "localhost",
        "protocol": "http",
        "log": 2,
        "module": {
            "load": [
                "demo",
                "app"
            ]
        },
        "event": {},
        "helper": {
            "logger": {
                "name": "ksmf",
                "type": "lib",
                "namespace": "app.Logger"
            },
            "MyAPI": {
                "path": "service",
                "module": "app"
            }
        }
    },
    "test": {
        "port": 3007,
        "host": "localhost",
        "protocol": "http",
        "log": 2,
        "module": {
            "load": [
                "demo",
                "app"
            ]
        },
        "event": {},
        "helper": {
            "logger": {
                "name": "ksmf",
                "type": "lib",
                "namespace": "app.Logger"
            },
            "MyAPI": {
                "path": "service",
                "module": "app"
            }
        }
    }
}