{
  "default": {
    "envkey": "",
    "port": 3005,
    "host": "localhost",
    "protocol": "http",
    "import": {
      "db": "cfg/db.js",
      "log": "cfg/log.json",
      "constant": "cfg/constant.json"
    },
    "doc": {
      "url": "/api/v1/doc",
      "src": "cfg/doc.json"
    },
    "cors": ["https://app.forestadmin.com"],
    "module": {
      "load": ["cdm", "comment", "app"]
    },
    "event": {
      "onInitConfig": [
        "logger.wrapper",
        "error.wrapper",
        "dao.wrapper",
        "doc.wrapper"
      ],
      "onInitModules": ["dao.wrapper"],
      "onLoadModule": ["dao.wrapper", "doc.wrapper"],
      "onLoadedModules": ["dao.wrapper", "forest.wrapper"],
      "onInitApp": ["logger.wrapper"],
      "onInitCompleted": ["doc.wrapper"],
      "onStart": ["monitor.wrapper"],
      "onError": ["monitor.wrapper"]
    },
    "helper": {
      "utl": {
        "name": "ksmf",
        "type": "lib",
        "namespace": "app.Utl"
      },
      "dao": {
        "name": "ksmf",
        "type": "lib",
        "namespace": "dao.Sequelize",
        "dependency": {
          "helper": "helper"
        }
      },
      "dao.wrapper": {
        "name": "ksmf",
        "type": "lib",
        "params": {
          "exclude": ["forest"]
        },
        "namespace": "dao.Wrapper",
        "dependency": {
          "helper": "helper"
        }
      },
      "forest.wrapper": {
        "name": "forest",
        "type": "module",
        "dependency": {
          "helper": "helper"
        }
      },
      "doc.wrapper": {
        "name": "ksmf",
        "type": "lib",
        "namespace": "doc.Swagger",
        "dependency": {
          "helper": "helper"
        }
      },
      "error.wrapper": {
        "name": "ksmf",
        "type": "lib",
        "namespace": "monitor.Error",
        "dependency": {
          "helper": "helper"
        }
      },
      "logger.wrapper": {
        "name": "ksmf",
        "type": "lib",
        "namespace": "monitor.LoggerWrapper",
        "dependency": {
          "helper": "helper",
          "manager": {
            "name": "ksmf",
            "type": "lib",
            "namespace": "monitor.LoggerManager",
            "dependency": {
              "driver": {
                "name": "ksmf",
                "type": "lib",
                "namespace": "monitor.LoggerSimple"
              }
            }
          }
        }
      },
      "monitor.wrapper": {
        "name": "ksmf",
        "type": "lib",
        "namespace": "monitor.Manager",
        "dependency": {
          "helper": "helper"
        }
      },
      "data.service": {
        "name": "ksmf",
        "type": "lib",
        "namespace": "dao.DataService",
        "dependency": {
          "helper": "helper"
        }
      },
      "cdm.data": {
        "type": "raw",
        "data": {
          "model": {
            "User": "User",
            "Domain": "Domain",
            "Credential": "Credential",
            "CredentialState": "CredentialState"
          }
        }
      },
      "MyAPI": {
        "path": "service",
        "module": "app"
      },
      "SrvAPI": {
        "path": "service",
        "module": "app"
      }
    }
  }
}
