{
  "name": "ksmf-skeleton-rest",
  "version": "1.0.1",
  "description": "This is a template to create REST API services in a simple and easy way",
  "main": "./bin/server.js",
  "scripts": {
    "start": "node node_modules/ksmf/bin/cli.js",
    "dev": "nodemon ./bin/server.js",
    "test": "jest",
    "tool": "node node_modules/ksmf/bin/cli.js",
    "db:model:auto": "node node_modules/ksmf/bin/cli.js db",
    "db:model:generate": "npx sequelize-cli model:generate --name ",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:status": "npx sequelize-cli db:migrate:status",
    "db:migrate:generate": "npx sequelize-cli migration:generate --name",
    "heroku-postbuild": "node_modules/.bin/sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ameksike/ksmf-skeleton-rest.git"
  },
  "keywords": [
    "ksmf",
    "rest",
    "api",
    "web",
    "ksike"
  ],
  "author": {
    "name": "Antonio Membrides Espinosa",
    "email": "tonykssa@gmail.com",
    "url": "https://github.com/ameksike"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ameksike/ksmf-skeleton-rest/issues"
  },
  "homepage": "https://github.com/ameksike/ksmf-skeleton-rest#readme",
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sqlite3": "^5.1.6",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "@forestadmin/agent": "^1.16.2",
    "@forestadmin/datasource-sequelize": "^1.5.0",
    "@forestadmin/datasource-sql": "^1.7.10",
    "axios": "^0.26.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.0",
    "ksmf": "^1.2.17",
    "mariadb": "^3.0.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.4.1",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0"
  }
}
